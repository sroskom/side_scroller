<FullImage>:
	canvas:
        Color:
            rgb: (1, 1, 1)
        Rectangle:
            texture: self.texture
            size: self.height*3,self.height
			pos: self.x,self.y
			
<TempBackground>:
	canvas:
        Color:
            rgb: (1, 1, 1)
        Rectangle:
            texture: self.texture
            size: self.height*3,self.height
			pos: self.x,self.y
<CusWid>:
	FullImage:
		id: img
		source: 'images/background_front.png'
		size: root.height*3,root.height
	FullImage:
		id: img2
		source: 'images/background_front2.png'
		size: root.height*3,root.height
	TempBackground:
		id: tmp1
		source: 'images/background_front_03.png'
		size: root.height*3,root.height
		pos: root.width,self.y
	Button:
		text: '+'
		font_size: self.size[0]
		size: root.height*.15, self.size[0]
		pos: root.width*.01, root.height-self.size[0]-self.pos[0]
		on_press: root.buttonPress()
	